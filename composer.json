{
    "name": "coinphon/coinphon",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": {
        "bitcoin": {
            "type": "path",
            "url": "packages/coinphon/crypto/src",
            "options": {
                "symlink": true
            }

        }
    },
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "aws/aws-sdk-php": "^3.171",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "bitwasp/bitcoin": "^1.0.4",
        "composer/composer": "^2.0",
        "consoletvs/charts": "7.*",
        "doctrine/dbal": "^3.1.4",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "glorand/laravel-model-settings": "^4.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/routing": "^8.29",
        "jenssegers/agent": "^2.6",
        "kielabokkie/bitcoin-address-validator": "^2.0",
        "laravel/framework": "^8.40",
        "laravel/octane": "^1.0",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.1",
        "paragonie/random-lib": "^2.0",
        "predis/predis": "^1.1",
        "twilio/sdk": "^6.23",
        "tymon/jwt-auth": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.9",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "knuckleswtf/scribe": "^2.5",
        "laravel/horizon": "^5.7",
        "laravel/sail": "^0.0.5",
        "laravel/telescope": "^4.5",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/phpinsights": "^2.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "barryvdh/laravel-ide-helper",
                "laravel/telescope"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "CoinPhon\\Crypto\\": "packages/coinphon/crypto/src",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
